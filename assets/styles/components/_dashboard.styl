.dashboard
  width 100%
  overflow-x hidden

  &__list
    margin 0
    padding 0
    list-style-type none
    clearfix()


.dashboard__listItem
  float left
  padding-bottom $height_controller
  height 550px
  overflow-y scroll
  -webkit-overflow-scrolling touch

  &Image
    line-height 0

    img
      width 100%
      height auto