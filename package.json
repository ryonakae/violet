{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "db": "mkdir -p db && mongod --nojournal --noprealloc --dbpath db",
    "browserify": "mkdir -p assets/javascripts && browserify client/javascripts/app.jsx -t babelify -o assets/javascripts/bundle.js",
    "start": "npm run browserify && nodemon ./app.js --exec babel-node"
  },
  "dependencies": {
    "babel": "^5.8.29",
    "body-parser": "~1.13.2",
    "connect-mongo": "^0.8.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-session": "^1.12.0",
    "jade": "~1.11.0",
    "method-override": "^2.3.5",
    "mongoose": "^4.2.3",
    "morgan": "~1.6.1",
    "passport": "0.2.2",
    "passport-tumblr": "^0.1.2",
    "passport.socketio": "3.5.1",
    "react": "0.13.3",
    "react-router": "0.13.3",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "tumblr.js": "0.0.5",
    "util": "^0.10.3"
  },
  "devDependencies": {
    "babelify": "^6.4.0",
    "browserify": "^12.0.0",
    "nodemon": "^1.8.0"
  }
